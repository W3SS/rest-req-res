{
  "app": {
    "version": "v1",
    "port": 3000,
    "host": "127.0.0.1",
    "jsonSpaces": 4,
    "origin": [
      "http://localhost:3000",
      "CurlTest"
    ]
  },
  "cors": {
    "maxAge": 86400,
    "allowHeaders": [
      "Content-Type",
      "Accept",
      "Origin",
      "X-HTTP-Method-Override",
      "User-Agent",
      "Authorization",
      "Cache-Control",
      "Keep-Alive",
      "X-Requested-With",
      "If-Modified-Since"
    ],
    "allowMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS"
    ]
  }
}
